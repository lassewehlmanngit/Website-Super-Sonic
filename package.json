{
  "name": "norddorf",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "tinacms dev -c \"vite\"",
    "dev:server": "cd server && npm run dev",
    "dev:all": "concurrently \"npm run dev\" \"npm run dev:server\"",
    "build:client": "vite build --outDir dist/static",
    "build:server": "vite build --ssr src/entry-server.tsx --outDir dist/server",
    "generate": "node prerender.js",
    "build": "tinacms build && npm run build:client && npm run build:server && npm run generate",
    "preview": "vite preview"
  },
  "dependencies": {
    "@react-pdf/renderer": "^4.3.1",
    "clsx": "^2.1.1",
    "lucide-react": "^0.556.0",
    "react": "^19.2.1",
    "react-dom": "^19.2.1",
    "react-router-dom": "^7.10.1",
    "tailwind-merge": "^3.4.0",
    "tinacms": "^3.4.1"
  },
  "devDependencies": {
    "@tailwindcss/vite": "^4.1.18",
    "@tinacms/cli": "^2.1.5",
    "@types/node": "^22.14.0",
    "@vitejs/plugin-react": "^5.0.0",
    "autoprefixer": "^10.4.23",
    "concurrently": "^8.2.2",
    "postcss": "^8.5.6",
    "tailwindcss": "^4.0.0",
    "terser": "^5.44.1",
    "typescript": "~5.8.2",
    "vite": "^6.2.0"
  },
  "overrides": {
    "graphql": "15.8.0"
  }
}
